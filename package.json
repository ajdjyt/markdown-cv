{
  "name": "@ajdjyt/markdown-cv",
  "version": "2024.0.0",
  "description": "Arnav Fernandes â€“ Curriculum Vitae",
  "author": "Arnav Fernandes <ajdjking@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajdjyt/markdown-cv.git"
  },
  "keywords": [
    "cv",
    "resume",
    "webdeveloper"
  ],
  "homepage": "https://cv.arjf.me/",
  "private": true,
  "scripts": {
    "start": "sirv public",
    "clean": "rm -rf public/build/",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "dev": "npm run build -- -w",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "postversion": "echo $(git push --tags)",
    "build-readme": "node ./scripts/build-readme.cjs",
    "deploy": "npm build && npx gh-pages -d build -t true"
  },
  "dependencies": {
    "figlet": "^1.6.0",
    "reset.css": "^2.0.2",
    "sirv-cli": "^2.0.2",
    "vercel": "^33.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "autoprefixer": "^10.4.15",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "rollup": "^3.29.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^5.0.4",
    "svelte-preprocess-markdown": "^2.7.3"
  },
  "browserslist": "last 2 versions"
}
